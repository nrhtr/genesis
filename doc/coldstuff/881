-----
&gt;arg l , [dummy];
&gt;var x,y,temp;
&gt;
&gt;if (type(l) != 'list){
&gt;   throw (~type, "Must be list of strings");}
&gt;for x in (substitutions)
&gt;    if (.find_method(x[2]))
&gt;        for y in [1 .. l.length()]  {
&gt;             temp = tosym(x[2]);
&gt;             temp =  (| .(temp)() |);
&gt;             l[y] = $string.replace(l[y],x[1],temp) ;
&gt;}
&gt;return l;

